{
  "name": "sunseekerx",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "serve": "vuepress dev docs no-cache",
    "build": "vuepress build docs",
    "lint:prettier": "prettier --write --loglevel warn \"./**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "gc": "git add -A && git-cz && git pull && git push",
    "up": "yarn upgrade-interactive",
    "pkg:sort": "npx sort-package-json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "husky": "^4.3.8",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "sort-package-json": "^1.50.0",
    "vuepress": "^1.8.2",
    "vuepress-theme-hope": "^1.18.0"
  }
}
